{
    "manifest_version": 2,

    "name": "Koreanize",
    "description": "Converts romanized korean text to hangul characters.",
    "version": "1.0",

  "browser_action": {
    "default_icon": "images/icon_48.png",
    "default_popup": "popup.html",
    "default_title": "Koreanize Text"
  },

  "icons": {
    "128": "images/icon_128.png",
    "48": "images/icon_48.png",
    "16": "images/icon_16.png"
  }
}